sed -i s/"\\\\beq"/"\\\\begin{equation}"/g
sed -i s/"\\\\eeq"/"\\\\end{equation}"/g

sed -i s/"\\\\bea"/"\\\\begin{eqnarray}"/g
sed -i s/"\\\\eea"/"\\\\end{eqnarray}"/g

#make list of substitutions for labelled equations

grep "\\fs{.*}" algorithms.tex | sed s/".*{"// | sed s/"}.*"// | awk '{text=$1 ; printf("%s", "s/\"\\\\\\fs{" text "}\"/\"\\\\\\\\begin{equation} \\\\\\\\label{eqn:" text "}\"/\n")}'

sed -i s/"\\\\fe"/"\\\\end{equation}"/g

